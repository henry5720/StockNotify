{
    "parser": "@babel/eslint-parser",
    "env": {
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:json/recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "2023",
        "sourceType": "module",
        "requireConfigFile": false,
        "babelOptions": {
            "presets": ["@babel/preset-env"],
            "plugins": ["@babel/plugin-syntax-import-assertions"]
        }
    },
    "plugins": ["json", "prettier"],
    "rules": {}
}
